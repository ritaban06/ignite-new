# For local development, comment out or remove VITE_API_URL to use Vite proxy
# VITE_API_URL=your-backend-url/api

# Or explicitly set to local backend:
VITE_API_URL=http://localhost:5000/api

# Google OAuth Client IDs for multi-platform support
# Web application Google OAuth Client ID (for browser/web platform)
VITE_GOOGLE_CLIENT_ID=your-google-oauth-web-client-id-here

# Android application Google OAuth Client ID (for mobile app platform)
VITE_GOOGLE_ANDROID_CLIENT_ID_DEBUG=your-google-oauth-android-debug-client-id-here
VITE_GOOGLE_ANDROID_CLIENT_ID_RELEASE=your-google-oauth-android-release-client-id-here

# Set this when building release (True for production builds & False for development builds)
RELEASE=true or false
